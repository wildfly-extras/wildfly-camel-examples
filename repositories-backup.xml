        <pluginRepository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>jboss-releases</id>
          <name>jboss-releases</name>
          <url>https://repository.jboss.org/nexus/content/repositories/releases/</url>
        </pluginRepository>
      </pluginRepositories>
      <id>removed-by-pme</id>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>removed-by-pme</activeProfile>
  </activeProfiles>
</settings>
